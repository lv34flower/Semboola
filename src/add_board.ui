<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.1 -->
<interface>
  <!-- interface-name add_board.ui -->
  <requires lib="gtk" version="4.6"/>
  <requires lib="libadwaita" version="1.7"/>
  <requires lib="gio" version="2.0"/>
  <template class="AddBoardWindow" parent="AdwApplicationWindow">
    <property name="default-height">160</property>
    <property name="default-width">360</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel">
            <property name="label" translatable="yes">Add using URL</property>
            <property name="margin-top">6</property>
            <property name="natural-wrap-mode">word</property>
            <property name="vexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="entry">
            <property name="margin-end">5</property>
            <property name="margin-start">5</property>
            <property name="valign">center</property>
            <property name="vexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <child>
              <object class="GtkButton" id="button_cancel">
                <property name="halign">end</property>
                <property name="hexpand">True</property>
                <property name="label" translatable="yes">Cancel</property>
                <property name="valign">center</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_submit">
                <property name="halign">center</property>
                <property name="label" translatable="yes">Add</property>
                <property name="margin-end">16</property>
                <property name="margin-start">8</property>
                <property name="valign">center</property>
                <property name="vexpand">True</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
  <menu/>
</interface>
