<interface>
  <!-- interface-name add_board.ui -->
  <requires lib="gtk" version="4.6"/>
  <requires lib="libadwaita" version="1.7"/>
  <template class="AddBoardWindow" parent="AdwApplicationWindow">
    <property name="default-height">200</property>
    <property name="default-width">360</property>
    <property name="resizable">False</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel">
            <property name="label" translatable="yes">Add using URL</property>
            <property name="margin-top">6</property>
            <property name="natural-wrap-mode">word</property>
            <property name="vexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="bdadd_entry">
            <property name="margin-end">5</property>
            <property name="margin-start">5</property>
            <property name="valign">center</property>
            <property name="vexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="bdadd_button_submit">
            <property name="halign">center</property>
            <property name="label" translatable="yes">Submit</property>
            <property name="valign">center</property>
            <property name="vexpand">True</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
