<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.3 -->
<interface>
  <!-- interface-name ng_row.ui -->
  <requires lib="gtk" version="4.18"/>
  <template class="ng_row" parent="GtkBox">
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkLabel" id="label_text">
        <property name="halign">start</property>
        <property name="hexpand">True</property>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <child>
          <object class="GtkLabel" id="label_enable">
            <property name="css-classes">dim-label</property>
            <property name="halign">start</property>
            <property name="hexpand">True</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="label_board">
            <property name="css-classes">dim-label</property>
            <property name="halign">end</property>
            <property name="margin-end">4</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="label_regex">
            <property name="css-classes">dim-label</property>
            <property name="halign">end</property>
            <property name="label" translatable="yes">[Regex]</property>
            <property name="margin-end">4</property>
            <property name="xalign">0.0</property>
            <property name="yalign">0.0</property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
