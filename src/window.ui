<interface>
  <!-- interface-name window.ui -->
  <requires lib="gio" version="2.0"/>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.7"/>
  <template class="SemboolaWindow" parent="AdwApplicationWindow">
    <property name="default-height">960</property>
    <property name="default-width">480</property>
    <property name="title" translatable="yes">Semboola</property>
    <child>
      <object class="AdwToastOverlay" id="toast">
        <child>
          <object class="AdwNavigationView" id="nav"/>
        </child>
      </object>
    </child>
  </template>
</interface>
